{
  "name": "stream-repro",
  "version": "0.1.0",
  "description": "Reproduction test case of issues in http streams",
  "type": "module",
  "scripts": {
    "start": "node --max-old-space-size=500 --watch app.js",
    "start-dev": "npm start",
    "test": "NODE_ENV=test npx mocha",
    "pretest": "npm run initdb",
    "initdb": "node initdb.js"
  },
  "mocha": {
    "exit": true,
    "bail": true,
    "check-leaks": true,
    "slow": 6000,
    "colors": true,
    "require": "mocha-suppress-logs",
    "global": [
      "statement_timeout"
    ],
    "spec": [
      "test/*/*.spec.js"
    ]
  },
  "engines": {
    "npm": "10.x",
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswmin/stream-repro.git"
  },
  "keywords": [
    "streams",
    "test-case",
    "repro",
    "issue",
    "http"
  ],
  "author": "@nicholaswmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicholaswmin/stream-repro/issues"
  },
  "homepage": "https://github.com/nicholaswmin/stream-repro#readme",
  "dependencies": {
    "express": "^4.19.2",
    "express-list-routes": "^1.2.1",
    "JSONStream": "^1.3.5",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "pg-query-stream": "^4.5.5"
  },
  "devDependencies": {
    "asciichart": "^1.5.25",
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "execa": "^9.1.0",
    "mocha": "^10.4.0",
    "mocha-suppress-logs": "^0.5.1",
    "prettyoutput": "^1.2.0",
    "single-line-log": "^1.1.2",
    "strip-ansi": "^7.1.0",
    "superagent-binary-parser": "^1.0.1"
  },
  "overrides": {
    "chai-http": {
      "superagent": "^9.0.2"
    }
  }
}
