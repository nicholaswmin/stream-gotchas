{
  "name": "stream-repro",
  "version": "0.1.0",
  "description": "Reproduction test case of issues in http streams",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "test": "npm run initdb && npx mocha test --exit --colors",
    "initdb": "node setup.js",
    "preinitdb": "dropdb -h localhost repro; createdb -h localhost repro"
  },
  "engines": {
    "npm": "10.x",
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswmin/stream-repro.git"
  },
  "keywords": [
    "streams",
    "test-case",
    "repro",
    "issue",
    "http"
  ],
  "author": "@nicholaswmin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicholaswmin/stream-repro/issues"
  },
  "homepage": "https://github.com/nicholaswmin/stream-repro#readme",
  "dependencies": {
    "express": "^4.19.2",
    "JSONStream": "^1.3.5",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "pg-query-stream": "^4.5.5"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "mocha": "^10.4.0",
    "superagent-binary-parser": "^1.0.1"
  },
  "overrides": {
    "chai-http": {
      "superagent": "^9.0.2"
    }
  }
}
